{
	"Logging": {
		"LogLevel": {
			"Default": "Information",
			"Microsoft": "Warning",
			"Microsoft.AspNetCore.HttpLogging": "Information",
			"Microsoft.Hosting.Lifetime": "Information"
		},
		"Console": {
			"FormatterName": "json",
			"FormatterOptions": {
				"IncludeScopes": true,
				"TimestampFormat": "yyyy-MM-ddTHH:mm:ss.fffZ",
				"UseUtcTimestamp": true,
				"JsonWriterOptions": {
					"Indented": false
				}
			}
		}
	}
}
